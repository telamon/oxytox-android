<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="se.decentlabs.oxytox.ui.party.PartyFragment"
        android:label="@string/title_party"
        tools:layout="@layout/party" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="se.decentlabs.oxytox.ui.booth.BoothFragment"
        android:label="@string/title_booth"
        tools:layout="@layout/booth" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="se.decentlabs.oxytox.ui.stats.StatsFragment"
        android:label="@string/title_stats"
        tools:layout="@layout/stats" />
</navigation>